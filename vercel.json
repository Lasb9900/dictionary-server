{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**/*.js"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
      }
    ]
  }